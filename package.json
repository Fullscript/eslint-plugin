{
  "name": "@fullscript/eslint-plugin",
  "version": "2.1.0",
  "description": "Fullscript custom eslint rules",
  "main": "dist/index.js",
  "contributors": [
    "Hossam Mourad",
    "Ryan O'Connor <charle692@gmail.com>",
    "Aidan Feuerherm <aidan.feuerherm@fullscript.com>",
    "Valentyn Patsera <valentyn.patsera@fullscript.com>",
    "Omar Nasr <omar.nasr@fullscript.com>",
    "Asuka Kuwahara <asuka.kuwahara@fullscript.com>"
  ],
  "license": "MIT",
  "private": false,
  "scripts": {
    "prettier-format": "prettier --loglevel 'warn' --write './src/**/*.js'",
    "prettier-diff": "prettier --loglevel 'warn' --check './src/**/*.js'",
    "build": "swc ./src -d dist",
    "build-watch": "swc ./src -d dist -w",
    "test": "jest ./src"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.20",
    "@swc/jest": "^0.2.23",
    "@typescript-eslint/utils": "^6.13.1",
    "chokidar": "^3.5.3",
    "core-js": "^3.26.1",
    "husky": "^8.0.0",
    "jest": "^29.3.1",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.0"
  }
}
